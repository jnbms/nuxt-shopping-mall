{
    "version": 2,
    "builds": [
    {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/vercel-builder",
        "config": {
        "serverFiles": ["package.json"]
        }
    }
    ],
    "routes": [],
    "rewrites": [
        {
            "source": "/:path*",
            "has": [
            {
                "type": "host",
                "value": "app.acme.com"
            }
            ],
            "destination": "/app/:path*"
        }
    ]
}